{
  "name": "last-green-commit-action",
  "version": "0.1.0",
  "private": true,
  "description": "Github Action to get the last green commit on your branch",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "yarn build && yarn format-check && yarn lint && yarn run pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/talentpair/last-green-commit-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Ben Brandt / Talentpair",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@actions/github": "^2.0.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^13.1.6",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.21.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-jest": "^23.4.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.6.4"
  },
  "volta": {
    "node": "12.14.1",
    "yarn": "1.21.1"
  }
}
