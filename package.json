{
  "name": "last-green-commit-action",
  "version": "0.1.0",
  "private": true,
  "description": "Github Action to get the last green commit on your branch",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "yarn build && yarn format-check && yarn lint && yarn run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/talentpair/last-green-commit-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Ben Brandt / Talentpair",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.3.0",
    "@actions/github": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "@vercel/ncc": "^0.28.6",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.3.0",
    "jest-circus": "^27.0.3",
    "js-yaml": "^4.1.0",
    "prettier": "^2.3.0",
    "ts-jest": "^27.0.2",
    "typescript": "^4.3.2"
  },
  "volta": {
    "node": "14.16.0",
    "yarn": "1.22.5"
  }
}
